<div class="bg-white py-3 px-5 shadow mt-5 rounded">
  <p id="notice"><%= notice %></p>
  <h1>Membership Withdrawals</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Content</th>
        <th>Status</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @membership_withdrawals.each do |membership_withdrawal| %>
        <tr>
          <td><%= membership_withdrawal.content %></td>
          <td><span class="status_tag <%= membership_withdrawal.status.to_sym %>"><%= membership_withdrawal.status %></span></td>
          <td><%= link_to 'Show', membership_withdrawal %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Membership Withdrawal', new_membership_withdrawal_path %>
</div>
